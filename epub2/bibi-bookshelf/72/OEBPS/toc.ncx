    <navPoint id="navPoint-47" playOrder="47">
      <navLabel>
        <text>Yeniçeri ihtilalleri</text>
      </navLabel>
      <content src="Text/Section0009.xhtml"/>
      <navPoint id="navPoint-48" playOrder="48">
        <navLabel>
          <text>İlk ayaklanma: Edirne’de Buçuktepe Vakası</text>
        </navLabel>
        <content src="Text/Section0017.xhtml"/>
      </navPoint>
      <navPoint id="navPoint-49" playOrder="49">
        <navLabel>
          <text>İstanbul’da ilk yeniçeri ihtilali</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_35"/>
      </navPoint>
      <navPoint id="navPoint-50" playOrder="50">
        <navLabel>
          <text>Yenibahçe Çayırı ayaklanması</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_36"/>
      </navPoint>
      <navPoint id="navPoint-51" playOrder="51">
        <navLabel>
          <text>Çaldıran seferindeki dört vaka</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_37"/>
      </navPoint>
      <navPoint id="navPoint-52" playOrder="52">
        <navLabel>
          <text>Bu ihtilallere rağmen ocak disiplini bozulmamıştır, yeniçeri cenk ateşinin semenderidir</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_38"/>
      </navPoint>
      <navPoint id="navPoint-53" playOrder="53">
        <navLabel>
          <text>Padişah fermanıyla bozulan ocak disiplini</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_39"/>
      </navPoint>
      <navPoint id="navPoint-54" playOrder="54">
        <navLabel>
          <text>Yemişçi Hasan Paşa Vakası</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_40"/>
      </navPoint>
      <navPoint id="navPoint-55" playOrder="55">
        <navLabel>
          <text>Genç Osman’a karşı 1622 ihtilali</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_41"/>
      </navPoint>
      <navPoint id="navPoint-56" playOrder="56">
        <navLabel>
          <text>İstanbul on yıl yeniçeri ve sipahi kılıcı altında yaşadı</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_42"/>
      </navPoint>
      <navPoint id="navPoint-57" playOrder="57">
        <navLabel>
          <text>Bağdat’ta Bekir Subaşı vakası</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_43"/>
      </navPoint>
      <navPoint id="navPoint-58" playOrder="58">
        <navLabel>
          <text>1632 ihtilali</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_44"/>
      </navPoint>
      <navPoint id="navPoint-59" playOrder="59">
        <navLabel>
          <text>İhtilal havası içinde hicrî 1041 ramazanı</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_45"/>
      </navPoint>
      <navPoint id="navPoint-60" playOrder="60">
        <navLabel>
          <text>Halep vakası</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_46"/>
      </navPoint>
      <navPoint id="navPoint-61" playOrder="61">
        <navLabel>
          <text>1648 ihtilali</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_47"/>
      </navPoint>
      <navPoint id="navPoint-62" playOrder="62">
        <navLabel>
          <text>Yeniçeriler ile sipahiler arasında Atmeydanı cengi</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_48"/>
      </navPoint>
      <navPoint id="navPoint-63" playOrder="63">
        <navLabel>
          <text>Yeniçeri ağaları saltanatı</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_49"/>
      </navPoint>
      <navPoint id="navPoint-64" playOrder="64">
        <navLabel>
          <text>1687 Varadin isyanı ve İstanbul ihtilali</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_50"/>
      </navPoint>
      <navPoint id="navPoint-65" playOrder="65">
        <navLabel>
          <text>1687 ihtilalinin kasım kargaşalıkları</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_51"/>
      </navPoint>
      <navPoint id="navPoint-66" playOrder="66">
        <navLabel>
          <text>1703 Edirne Vakası</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_52"/>
      </navPoint>
      <navPoint id="navPoint-67" playOrder="67">
        <navLabel>
          <text>1730 ihtilali ve yalın ayaklılar saltanatı</text>
        </navLabel>
        <content src="Text/Section0017.xhtml#sigil_toc_id_53"/>
      </navPoint>
    </navPoint>
    <navPoint id="navPoint-68" playOrder="68">
      <navLabel>
        <text>Son yeniçeriler</text>
      </navLabel>
      <content src="Text/Section0010.xhtml"/>
      <navPoint id="navPoint-69" playOrder="69">
