        <content src="Text/Section0018.xhtml#sigil_toc_id_60"/>
      </navPoint>
      <navPoint id="navPoint-77" playOrder="77">
        <navLabel>
          <text>1807 ihtilali</text>
        </navLabel>
        <content src="Text/Section0018.xhtml#sigil_toc_id_61"/>
      </navPoint>
      <navPoint id="navPoint-78" playOrder="78">
        <navLabel>
