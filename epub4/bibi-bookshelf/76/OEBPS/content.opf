    <dc:title>Dr. Moreau’nun Adası</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Wells, H. G.">H. G. Wells</dc:creator>
    <dc:identifier id="uuid">{12499E0C-B8FC-4BE2-BF66-6AB84CD4A3EC}</dc:identifier>
